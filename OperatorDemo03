/*
字符串的“+”操作
 */
public class OperatorDemo03 {
    public static void main(String[] args){
        System.out.println("Calvin"+"Tse");
        System.out.println("Calvin"+666);
        System.out.println(666+"Calvin");
        System.out.println("Calvin"+66+6);
        System.out.println(1300+14+"Calvin");
    }
}
